@page "{id?}"
@using DomainRegistry.Models
@model DomainRegistry.Pages.DomainInformationModel
@{
    ViewData["Title"] = "DomainInformation";
}

<div class="container">
    <div class="row">
        <div class="col-sm">
            <div class="row">
                <h4>Domain Name:&nbsp;&nbsp;</h4>
                <p> @Model.Domain.DomainName</p>
            </div>
            <div class="row">
                <h4>Verification Provider:&nbsp;&nbsp;</h4>
                <p> Provider- @Model.Domain.Provider</p>
            </div>
            <div class="row">
                <h4>Period of Registration:&nbsp;&nbsp;</h4>
                <p> @Model.Domain.Period year(s)</p>
            </div>
        </div>
    </div>
</div>
